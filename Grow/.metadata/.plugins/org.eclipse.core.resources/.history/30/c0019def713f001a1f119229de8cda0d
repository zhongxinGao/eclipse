package com.data;

public class Text {
	public static void main(String[] args) {
	double a = Math.pow(0.99, 365);
	double b = Math.pow(0.1, 365);
	double c = Math.pow(1.11, 365);
	double d = Math.pow(1.12, 365);
	System.out.println(a);
	System.out.println(b);
	System.out.println(c);
	System.out.println(d);
}
}
